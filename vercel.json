{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": [
        "GET",
        "POST",
        "DELETE",
        "PUT"
      ]
    }
  ],
  "buildCommand": "sh vercel.sh"
}
